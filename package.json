  "lint-staged": {
    "*.conf": [
      "pnpm nginxbeautifier -s 4",
      "pnpm nginx-linter --include"
    ],
    "*.css": [
      "pnpm prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "pnpm prettier --write",
      "pnpm eslint --fix"
    ],
    "*.{json,md}": [
      "pnpm prettier --write"
    ]
  },
  "name": "jacobjove",
  "prettier": {
    "printWidth": 110,
    "proseWrap": "always"
  },
  "private": true,
  "scripts": {
    "compile": "tsx scripts/compile.ts",
    "build": "pnpm compile && dotenv -- next build",
    "dev": "docker compose up -d tempo && PORT=$(grep '^PORT=' .env | cut -d '=' -f2) next dev",
    "format": "pnpm prettier -w . && pnpm eslint --fix .",
    "lint": "next lint",
    "test": "jest --passWithNoTests",
    "typecheck": "tsc --noEmit",
    "prepare": "if [ ! $DOCKERIZED = true ]; then npx husky; fi"
  },
  "stylelint": {
    "extends": [
      "stylelint-config-standard",
      "stylelint-config-tailwindcss"
    ]
  },
  "version": "1.0.0",
  "packageManager": "pnpm@9.5.0",
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=11.0.0",
    "pnpm": ">=9.5.0"
  },
  "postcss": {
    "plugins": {
      "tailwindcss": {},
      "autoprefixer": {}
    }
  },
  "pnpm": {
    "supportedArchitectures": {
      "os": [
        "darwin",
        "linux"
      ],
      "cpu": [
        "x64",
        "arm64"
      ]
    }
